{
    "manifest_version": 3,
    "name": "Budget Manager",
    "version": "1.0.0",
    "description": "This Extension Going to Track Spending of a Person.",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}